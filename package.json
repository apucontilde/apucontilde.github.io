  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "5.0.0-alpha.156",
    "@mui/material": "^5.15.0",
    "@mui/system": "^5.15.0",
    "@mui/utils": "^5.15.0",
    "@types/suncalc": "^1.9.2",
    "is-mobile": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-globe.gl": "^2.27.2",
    "react-helmet-async": "^2.0.3",
